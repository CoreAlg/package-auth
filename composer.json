    "require": {
        "php": ">=7.0",
        "corealg/helper": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CoreAlg\\Auth\\": "src",
            "CoreAlg\\Auth\\Tests\\": "tests"
        }
